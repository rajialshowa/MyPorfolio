@model IEnumerable<Core.Entities.AboutMe>

@{
    ViewData["Title"] = "Index";
    Layout = "_AdminLayout";
}
    <div style="padding:160px;">
        <h1>Index</h1>

        @*<p>
            <a asp-action="Create" class="btn btn-primary">Create New</a>
        </p>*@
        <table class="table">
            <thead>
                <tr>
                    <th>
                       Left Section
                    </th>
                    <th>
                       Right Section
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Section1)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Section2)
                        </td>
                        <td>
                            <a asp-action="Edit" asp-route-id="@item.Id"><i class="fas fa-edit"></i></a> |
                            <a asp-action="Details" asp-route-id="@item.Id"><i class="fas fa-eye"></i></a>@* |
                            <a asp-action="Delete" asp-route-id="@item.Id"><i class="fas fa-trash"></i></a>*@
                        </td>
                    </tr>
                }
            </tbody>
        </table>
        </div>